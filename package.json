{
  "name": "@countergank/persistent",
  "version": "1.0.0",
  "description": "Persistent Management",
  "main": "lib/core.js",
  "types": "lib/core.d.ts",
  "module": "lib/core.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "prepublishOnly1": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/countergank/persistent.git"
  },
  "author": "Leandro J. Cepeda",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Countergank"
  ],
  "dependencies": {
    "@types/react": "^18.0.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "dotenv-webpack": "^8.0.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "@countergank:registry": "https://npm.pkg.github.com"
  }
}
